{
"Application": {
    "envConfig":{
        "port":"5600",
        "baseUrl":"/node-service",
        "logging":{
            "directory" : "logs/",
            "filename" : "sample-app.log",
            "datePattern":"YYYY-MM-DD",
            "level" : "debug"
        },
        "Eureka":{
            "client":{
                "app": "node-service",
              "hostName": "localhost",
              "ipAddr": "127.0.0.1",
              "port":{
                  "$": "5600",
                  "@enabled": "true"
              },      
              "statusPageUrl": "http://localhost:5600/node-service/status",
              "vipAddress": "node-service",
              "dataCenterInfo": {
                "name": "MyOwn",
                "@class":"com.netflix.appinfo.InstanceInfo$DefaultDataCenterInfo"
              },
              "registerWithEureka": true,
              "fetchRegistry": true
            },
            "server":{
                "host": "localhost",
              "port": "8761",
              "servicePath": "/eureka/apps"
            }
        }
    }
}
}